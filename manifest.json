{
    "manifest_version": 2,
    "name": "RightMoveTing",
    "version": "2.0",
    "permissions": [
      "storage"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": [
            "https://www.rightmove.co.uk/property-to-rent/find.html?*", 
            "https://rightmove.co.uk/property-to-rent/find.html?*"
          ],
          "js": ["config.js", "content.js"]
        }
      ]
  }
